{
  "name": "PostMedia",
  "base": "PersistedModel",
  "indexes": {},
  "plural": "post-media",
  "strict": "filter",
  "idInjection": false,
  "forceId": true,
  "options": {
    "validateUpsert": true
  },
  "replaceOnPUT": true,
  "properties": {
    "path": {
      "type": "String",
      "required": true
    },
    "createdBy": {
      "type": "String",
      "required": true
    },
    "createdAt": {
      "type": "Date",
      "required": true
    },
    "deletedAt": {
      "type": "Date"
    },
    "deletedBy": {
      "type": "String"
    },
    "isReported": {
      "type": "Boolean",
      "required": true,
      "default": false
    }
  },
  "validations": [],
  "relations": {
    "userCreated": {
      "type": "belongsTo",
      "model": "WeflexUser",
      "foreignKey": "createdBy",
      "options": {
        "validate": true
      }
    },
    "userDeleted": {
      "type": "belongsTo",
      "model": "WeflexUser",
      "foreignKey": "deletedBy",
      "options": {
        "validate": true
      }
    },
    "post": {
      "type": "belongsTo",
      "model": "Post",
      "foreignKey": "postId",
      "options": {
        "validate": true,
        "forceId": false
      }
    },
    "file": {
      "type": "belongsTo",
      "model": "files",
      "foreignKey": "filesId",
      "options": {
        "validate": true,
        "forceId": false
      }
    }
  },
  "acls": [
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "$everyone",
      "permission": "DENY"
    },
    {
      "accessType": "READ",
      "principalType": "ROLE",
      "principalId": "$authenticated",
      "permission": "ALLOW"
    },
    {
      "accessType": "WRITE",
      "principalType": "ROLE",
      "principalId": "$authenticated",
      "permission": "ALLOW"
    }
  ],
  "mixins": {
    "StoreRelatedModel": true
  }
}
